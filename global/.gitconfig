[user]
    name = ErikBoesen
    email = me@erikboesen.com
[credential]
	helper = store
[core]
    whitespace = warn
    autocrlf = input
    excludesfile = ~/.gitignore
[github]
	user = ErikBoesen
[url "ssh://git@github.com/"]
    insteadOf = https://github.com/
[pull]
	rebase = false
[filter "lfs"]
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
